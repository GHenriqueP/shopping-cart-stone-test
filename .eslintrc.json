{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
        "error",
        2
    ],
    "linebreak-style": [
        "error",
        "unix"
    ],
    "semi": [
        "error",
        "always"
    ],
    "jsx-quotes": [
        2,
        "prefer-double"
    ],
    "eol-last": [
        "error",
        "always"
    ]
  }
}
